using System;

namespace ConsoleApp1
{
    internal class ConsoleApp1
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int a, b, c;
            a = n / 100;
            b = n % 100 / 10;
            c = n % 10;
            int res = c * 100 + b * 10 + a;
            Console.WriteLine(res);
        }
    }
}
